{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "semi": [2, "never"],
    "no-unexpected-multiline": 2,
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "es5",
      "bracketSpacing": true,
      "jsxBracketSameLine": true,
      "parser": "babylon",
      "semi": false
    }]
  }
}
